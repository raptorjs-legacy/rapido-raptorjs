{
    "scaffold.component.dir": "scaffolds/component",
    "scaffold.library.dir": "scaffolds/library",
    "scaffold.module.dir": "scaffolds/module",
    "scaffold.page.dir": "scaffolds/page",
    "scaffold.webapp.dir": "scaffolds/webapp",
    "scaffold.tag.dir": "scaffolds/tag",
    "scaffold.taglib.dir": "scaffolds/taglib",
    "scaffold.template.dir": "scaffolds/template",

    "name": "raptorjs",
    "description": "Commands for the RaptorJS Toolkit",
    
    "commands": {
        "create component": {
            "description": "Creates a new RaptorJS UI component",
            "file": "commands/command-create-component.js"
        },
        "create page": {
            "description": "Creates a new RaptorJS page",
            "file": "commands/command-create-page.js"
        },
        "create webapp": {
            "description": "Creates a new RaptorJS webapp",
            "file": "commands/command-create-webapp.js"
        },
        "create static site": {
            "description": "Creates a new static website",
            "file": "commands/command-create-static-site.js"
        },
        "create template": {
            "description": "Creates a new Raptor Template file",
            "file": "commands/command-create-template.js"
        },
        "create taglib": {
            "description": "Creates a new Raptor Templates taglib",
            "file": "commands/command-create-taglib.js"
        },
        "create tag": {
            "description": "Creates a new Raptor Templates tag",
            "file": "commands/command-create-tag.js"
        },
        "create lib": {
            "description": "Creates a new shareable RaptorJS library",
            "file": "commands/command-create-lib.js"
        },
        "create module": {
            "description": "Creates a new RaptorJS module",
            "file": "commands/command-create-module.js"
        }
    }
}