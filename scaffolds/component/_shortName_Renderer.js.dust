define(
    '{name}/{shortName}Renderer',
    function(require) {
        var templating = require('raptor/templating');

        return {
            render: function(input, context) {
                templating.render('{name}', {
                    name: input.name,
                    count: input.count
                }, context);
            }
        };
        
        return ButtonTag;
    });